finaldf <- read.csv("C:\\Users\\midyav01\\OneDrive - The Mount Sinai Hospital\\MSSM Projects\\SHAP\\charge data\\finaldfDD.csv")
exposures <- colnames(finaldf)[11:72]

start.time <- Sys.time()

gg <- gwqs(DD ~ wqs + csex + crace + cage + YOB + OwnHome + MMC_4cat + AgeMomYrs,
             mix_name = exposures, data = finaldf, q = 10, signal = "t2",
             b = 250, rs = T,   n_vars = 12,  validation = 0, 
             seed = 123123123, 
             b1_pos = T,  family = "binomial",plan_strategy = "multicore")

end.time <- Sys.time()
(time.taken <- end.time - start.time)

finaldf$wqs_q_10 <- gg$wqs
